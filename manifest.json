{
	"manifest_version": 2,
	"name": "change user agent",
	"description": "change browser user agent",
	"version": "1.0",
	"permissions": [
		"storage",
		"webRequest",
		"webRequestBlocking",
		"http://*/",
		"https://*/"
	],
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	}
}
